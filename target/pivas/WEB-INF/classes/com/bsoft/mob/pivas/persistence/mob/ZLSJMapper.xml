<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bsoft.mob.pivas.persistence.mob.ZLSJMapper">

    <select id="getPCs" resultType="com.bsoft.mob.pivas.domain.mob.ZLSJ">
        SELECT JLXH , PCID , JPBH FROM JM_ZLSJ WHERE  JPBH =#{jpbh} AND PZBZ=1 AND ZFBZ=0 ORDER BY JLXH
    </select>
</mapper>