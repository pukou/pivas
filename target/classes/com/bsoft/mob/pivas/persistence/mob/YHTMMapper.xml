<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bsoft.mob.pivas.persistence.mob.YHTMMapper">


    <select id="getYHTM" resultType="com.bsoft.mob.pivas.domain.mob.YHTM" >
        SELECT TMBH,TMNR,YGDM,TMZT,SBXT,JGID FROM MOB_YHTM WHERE TMNR=#{barcode} AND TMZT=0
    </select>


</mapper>